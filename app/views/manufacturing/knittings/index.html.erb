<p id="notice"><%= notice %></p>

<h1>Manufacturing Knittings</h1>

<table>
  <thead>
    <tr>
      <th>Ordernumber</th>
      <th>Parts</th>
      <th>Quantity</th>
      <th>Worker</th>
      <th>Serial</th>
      <th>Status</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @manufacturing_knittings.each do |manufacturing_knitting| %>
      <tr>
        <td><%= manufacturing_knitting.ordernumber %></td>
        <td><%= manufacturing_knitting.parts %></td>
        <td><%= manufacturing_knitting.quantity %></td>
        <td><%= manufacturing_knitting.worker %></td>
        <td><%= manufacturing_knitting.serial %></td>
        <td><%= manufacturing_knitting.status %></td>
        <td><%= link_to 'Show', manufacturing_knitting %></td>
        <%-#=begin <td><%= link_to 'Edit', edit_manufacturing_knitting_path(manufacturing_knitting)</td> %>
        <td><%= link_to 'Destroy', manufacturing_knitting, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Manufacturing Knitting', new_manufacturing_knitting_path %>
