<p id="notice"><%= notice %></p>

<h1>Sizemasters</h1>

<table>
  <thead>
    <tr>
      <th>Size</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @sizemasters.each do |sizemaster| %>
      <tr>
        <td><%= sizemaster.size %></td>
        <td><%= link_to 'Show', sizemaster %></td>
        <td><%= link_to 'Edit', edit_sizemaster_path(sizemaster) %></td>
        <td><%= link_to 'Destroy', sizemaster, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Sizemaster', new_sizemaster_path %>
